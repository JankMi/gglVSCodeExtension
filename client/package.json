{
  "name": "ggl-client",
  "displayName": "ggl language support client",
  "description": "ggl language support ggl e.g. game generation language",
  "version": "0.0.1",
  "publisher": "JankMi",
  "engines": {
    "vscode": "^1.21.0"
  },
  "categories": [
    "Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "ggl",
        "aliases": [
          "ggl",
          "ggl"
        ],
        "extensions": [
          ".ggl"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "ggl",
        "scopeName": "",
        "path": "./syntaxes/ggl.tmLanguage.json"
      }
    ]
  },
  "dependencies": {
    "child-process": "^1.0.2",
    "child_process": "^1.0.2",
    "node": "^9.8.0",
    "path": "^0.12.7",
    "vscode": "^1.1.13",
    "vscode-languageclient": "^4.0.0"
  },
  "activationEvents": [
    "onLanguage:ggl"
  ],
  "author": "JankMi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JankMi/gglVSCodeExtension.git"
  },
  "bugs": {
    "url": "https://github.com/JankMi/gglVSCodeExtension/issues"
  },
  "homepage": "https://github.com/JankMi/gglVSCodeExtension#readme",
  "main": "index.js",
  "devDependencies": {},
  "scripts": {
    "test": "mocha"
  }
}
