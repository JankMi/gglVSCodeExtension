{
  "name": "genesisvscode",
  "displayName": "genesisVSCode",
  "description": "language services for the genesis ggl",
  "version": "0.1.5",
  "publisher": "JankMi",
  "engines": {
    "vscode": "^1.20.0"
  },
  "categories": [
    "Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "ggl",
        "aliases": [
          "ggl",
          "GameGenerationLanguge"
        ],
        "extensions": [
          ".ggl"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "snippets": [
      {
        "language": "ggl",
        "path": "./snippets/ggl.json"
      },
      {
        "language": "json",
        "path": "./snippets/json.json"
      }
    ],
    "grammars": [
      {
        "language": "ggl",
        "scopeName": "source.genesisGGL",
        "path": "./syntaxes/ggl.tmLanguage.json"
      }
    ],
    "configuration": [
      {
        "title": "gglConfiguration",
        "properties": {
          "gglConfiguration.gglVersion": {
            "type": "string",
            "default": "4.1",
            "description": "used to build the import path for stdgg, commons, ..."
          }
        }
      }
    ]
  },
  "main": "out/extension",
  "dependencies": {
    "cache-base": "^2.0.2",
    "child-process": "^1.0.2",
    "child_process": "^1.0.2",
    "expand-brackets": "^2.1.4",
    "fast-plist": "^0.1.2",
    "file-system": "^2.2.2",
    "gulp": "^3.9.1",
    "gyp": "^0.5.0",
    "node-gyp": "^3.6.2",
    "oniguruma": "^7.0.0",
    "orchestrator": "^0.3.8",
    "path": "^0.12.7",
    "unset-value": "^1.0.0",
    "vscode": "^1.1.10",
    "vscode-textmate": "^3.2.0",
    "walk-up": "^1.0.2"
  },
  "activationEvents": [
    "onLanguage:ggl"
  ],
  "devDependencies": {
    "@types/node": "^9.4.6",
    "windows-build-tools": "^2.2.1"
  },
  "scripts": {
    "vscode:prepublish": "sh ./scripts/prepublish.sh",
    "compile": "sh ./scripts/compile.sh",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "author": "JankMi",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/JankMi/gglVSCodeExtension"
  }
}
